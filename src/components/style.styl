.paper
  padding: 20px 20px
  text-align: left

.flex-around-center-wrap
  display: flex
  justify-content: space-around;
  flex-wrap: wrap;
  align-items: center;

.flex-x-space-evenly
  display: flex;
  width: 100%
  justify-content: space-evenly;

.demo-charts
  .example-body div
    height: 100%
    width: 100%
    display: flex
    justify-content: center;
    align-items: center;

pre
